"VertexLitGeneric"
{
	"$basetexture"	"models/player/demo_female_hhh/femme_headless_hatman_skin"
	"$surfaceprop"	"default"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" ".01"
	"$detailblendmode" 6
	
	"$translucent" "1"
	"$additive" "0"
	"$emissiveBlendEnabled"     "1" // Enables effect
	"$emissiveBlendTexture"     "models/vortigaunt/vortigaunt_illum"
	"$emissiveBlendBaseTexture" "Models/Vortigaunt/vortigaunt_blue"
	"$emissiveBlendFlowTexture" "models/vortigaunt/vortigaunt_flow"
	"$yellow" "0"
	"$one" "1"


// 	"$bbqamount" "1"	
// 	"$two" "2.35"
// 	"$one" "1"
// 	"$absburnminusone" "1"
// 	"$burnminusone" "1"
// 	"$finalburnlevel" "1"
// 	"$burnlevel" "0"


	"$phong" "1"
	"$phongexponent" "20"
	"$phongboost" ".3"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "0"

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "2"										// Boost for ambient cube component of rim lighting

	"$color2"				"{255 174 56}"
	"$blendtintbybasealpha"		"0"
	"$blendtintcoloroverbase"		"1"

	"$colortint_tmp"		"[0 0 0]"


	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
	}
}